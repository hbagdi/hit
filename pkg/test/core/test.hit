@_global
base_url=https://httpbin.org
version=1


@get-headers
GET
/headers
foo:bar


@populate-cache
POST
/anything
~y2j
string: foobar
bool-true: true
bool-false: false
num: 42
num-float: 42.42

@get-using-cache
GET
/anything
foo:bar
~hy2j
string: "@populate-cache.json.string"
bool-true: "@populate-cache.json.bool-true"
bool-false: "@populate-cache.json.bool-false"
num: "@populate-cache.json.num"
num-float: "@populate-cache.json.num-float"

@get-cache-ref-in-path
GET
/anything/@populate-cache.json.string

@get-cache-ref-in-query-param
GET
/anything/qp?foo=@populate-cache.json.num

@cli-arg-types
POST
/anything/@2
~hy2j
input: "@2"


@post-with-static-body
POST
/anything
~y2j
foo: bar
baz: buz